def plusMinus(arr):
    p=0
    n=0
    z=0
    l=len(arr)
    for i in arr:
        if i>0:
            p+=1
        elif i<0:
            n+=1
        elif i==0:
            z+=1
    print(p/l)
    print(n/l)
    print(z/l)
    
n = int(input().strip())
arr = list(map(int, input().rstrip().split()))
plusMinus(arr)
